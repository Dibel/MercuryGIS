<Window x:Class="MercuryGIS.NewField"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MercuryGIS"
        mc:Ignorable="d"
        Title="新建字段" Height="290" Width="300">
    <Grid>
        <Label x:Name="label" Content="字段名" HorizontalAlignment="Left" Margin="30,35,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="120,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="字段类型" HorizontalAlignment="Left" Margin="30,115,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="120,115,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnCancel" Content="取消" HorizontalAlignment="Left" Margin="210,234,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Button x:Name="btnOk" Content="确定" HorizontalAlignment="Left" Margin="105,234,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>

    </Grid>
</Window>
