<Window x:Class="MercuryGIS.Coord"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MercuryGIS"
        mc:Ignorable="d"
        Title="请输入坐标" Height="155" Width="332.5">
    <Grid>
        <Button x:Name="btnCancel" Content="取消" HorizontalAlignment="Left" Margin="242,98,0,0" VerticalAlignment="Top" Width="76" Click="btnCancel_Click"/>
        <Button x:Name="btnOk" Content="确定" HorizontalAlignment="Left" Margin="140,98,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.133,0.368" Click="btnOk_Click"/>
        <Label x:Name="label" Content="X" HorizontalAlignment="Left" Margin="16,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="37,44,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Copy" Content="Y" HorizontalAlignment="Left" Margin="171,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="192,44,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
