<Window x:Class="MercuryGIS.StartEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MercuryGIS"
        mc:Ignorable="d"
        Title="StartEdit" Height="500" Width="400">
    <Grid>
        <Label x:Name="label" Content="请选择要编辑的图层：" HorizontalAlignment="Left" Margin="31,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.778,0.82"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="325" Margin="31,55,0,0" VerticalAlignment="Top" Width="335"/>
        <Button x:Name="btnCancel" Content="取消" HorizontalAlignment="Left" Margin="286,427,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Button x:Name="btnOk" Content="确定" HorizontalAlignment="Left" Margin="181,427,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>

    </Grid>
</Window>
